    {
      "cell_type": "code",
      "source": [
        "import pandas as pd\n",
        "import numpy as np\n",
        "\n",
        "# Load dataset\n",
        "data = pd.read_csv('/content/drive/MyDrive/CODE/homeprices (1).csv')\n",
        "\n",
        "# Display basic dataset information\n",
        "print(\"Initial Data Overview:\")\n",
        "print(data.info())\n",
        "\n",
        "# Handling missing values\n",
        "# Fill missing numerical values with mean\n",
        "data.fillna({\n",
        "    col: data[col].mean() for col in data.select_dtypes(include=['float64', 'int64']).columns\n",
        "}, inplace=True)\n",
        "\n",
        "# Fill missing categorical values with mode\n",
        "for col in data.select_dtypes(include=['object']).columns:\n",
        "    data[col].fillna(data[col].mode()[0], inplace=True)\n",
        "\n",
        "# Standardize text columns\n",
        "for col in data.select_dtypes(include=['object']).columns:\n",
        "    data[col] = data[col].str.strip().str.lower()\n",
        "\n",
        "# Remove duplicates\n",
        "data.drop_duplicates(inplace=True)\n",
        "\n",
        "# Convert date columns to datetime\n",
        "date_columns = [col for col in data.columns if 'date' in col.lower()]\n",
        "for col in date_columns:\n",
        "    data[col] = pd.to_datetime(data[col], errors='coerce')\n",
        "\n",
        "# Detect and handle outliers using IQR\n",
        "for col in data.select_dtypes(include=['float64', 'int64']).columns:\n",
        "    Q1 = data[col].quantile(0.25)\n",
        "    Q3 = data[col].quantile(0.75)\n",
        "    IQR = Q3 - Q1\n",
        "    data = data[~((data[col] < (Q1 - 1.5 * IQR)) | (data[col] > (Q3 + 1.5 * IQR)))]\n",
        "\n",
        "# Rename columns for consistency\n",
        "data.columns = data.columns.str.strip().str.lower().str.replace(' ', '_')\n",
        "\n",
        "# Normalize numerical columns\n",
        "data['normalized_column'] = (data['numeric_column'] - data['numeric_column'].min()) / (\n",
        "    data['numeric_column'].max() - data['numeric_column'].min()\n",
        ")\n",
        "\n",
        "# Save cleaned dataset\n",
        "data.to_csv('cleaned_dataset_advanced.csv', index=False)\n",
        "\n",
        "print(\"Advanced data cleaning completed. Cleaned data saved as 'cleaned_dataset_advanced.csv'.\")"
      ],
