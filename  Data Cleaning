        "import pandas as pd\n",
        "import numpy as np\n",
        "\n",
        "# Load dataset\n",
        "data = pd.read_csv('/content/drive/MyDrive/CODE/carprices.csv')\n",
        "\n",
        "# Display basic info about the dataset\n",
        "print(\"\\nInitial Data Overview:\")\n",
        "print(data.info())\n",
        "\n",
        "# Handle missing values\n",
        "# 1. Remove rows with missing target values (if applicable)\n",
        "data.dropna(subset=['target_column'], inplace=True)\n",
        "\n",
        "# 2. Fill missing values with mean, median, or a placeholder\n",
        "numerical_columns = ['column1', 'column2']\n",
        "data[numerical_columns] = data[numerical_columns].fillna(data[numerical_columns].mean())\n",
        "data['categorical_column'] = data['categorical_column'].fillna('Unknown')\n",
        "\n",
        "# Remove duplicates\n",
        "data.drop_duplicates(inplace=True)\n",
        "\n",
        "# Handle incorrect or inconsistent data entries\n",
        "# Example: Standardize text data\n",
        "data['city'] = data['city'].str.strip().str.title()  # Remove extra spaces and capitalize\n",
        "\n",
        "# Convert data types if necessary\n",
        "data['date_column'] = pd.to_datetime(data['date_column'], errors='coerce')\n",
        "\n",
        "# Remove outliers using z-score (for example, on column 'column_name')\n",
        "from scipy import stats\n",
        "z_scores = np.abs(stats.zscore(data['column_name']))\n",
        "data = data[z_scores < 3]\n",
        "\n",
        "# Rename columns for consistency\n",
        "columns_rename_map = {'OldColumnName': 'NewColumnName'}\n",
        "data.rename(columns=columns_rename_map, inplace=True)\n",
        "\n",
        "# Normalize numerical columns (scaling)\n",
        "data['normalized_column'] = (data['column_to_normalize'] - data['column_to_normalize'].min()) / (\n",
        "    data['column_to_normalize'].max() - data['column_to_normalize'].min()\n",
        ")\n",
        "\n",
        "# Save the cleaned dataset\n",
        "data.to_csv('cleaned_dataset.csv', index=False)\n",
        "\n",
        "print(\"\\nData Cleaning Complete. Cleaned dataset saved as 'cleaned_dataset.csv'.\")\n"
      ]
